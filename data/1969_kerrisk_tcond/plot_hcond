#!/usr/bin/gnuplot

# 1W = 1e7 erg/s
# 1 dyn/cm2 = 0.1 Pa = 1e-6 bar

K4(t,p) = 1e7*(\
 - 1.4642e-5\
 + 8.8025e-5  *t\
 + 9.1386e-13 *p * 1e6\
 - 8.7000e-6  *t**2\
 - 4.1701e-20 *p**2 * 1e12\
 + 8.3023e-13 *t*p  * 1e6 )

K3(t,p) = 1e7*(\
+ 1.8501e-5\
+ 6.5501e-5  * t\
- 6.2602e-6  * t**2\
- 2.2759e-20 * p**2 * 1e12\
+ 8.2703e-13 * p*t * 1e6 )

###
K4_(t,p) = \
 - 1.4642e2\
 + 8.8025e2 *t\
 + 9.1386e0 *p\
 - 8.7000e1 *t**2\
 - 4.1701e-1 *p**2\
 + 8.3023e0  *t*p

K3_(t,p) =\
+ 1.8501e2\
+ 6.5501e2  * t\
- 6.2602e1  * t**2\
- 2.2759e-1 * p**2\
+ 8.2703e0  * p*t

####

plot [1.5:4]\
  K3(x,0),\
  K3(x,10),\
  K3(x,20),\
\
  K3_(x,0)  lc 1,\
  K3_(x,10) lc 2,\
  K3_(x,20) lc 3,\

pause -1

plot [1.5:4]\
  K4(x,0)  lc 1,\
  K4(x,10) lc 2,\
  K4(x,20) lc 3,\
\
  K4_(x,0)  lc 1,\
  K4_(x,10) lc 2,\
  K4_(x,20) lc 3,\

pause -1
