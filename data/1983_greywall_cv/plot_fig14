#!/usr/bin/gnuplot

set xlabel "T, mK"
set ylabel "C_v/RT"

# pressure to Vm: he3_vm([32.5 29.30 22.22 17.01 11.00 5.040 0.060])
#    25.825   26.256   27.323   28.405   30.016   32.568   36.767

plot [0:50] [2.5:4.5]\
 "< ../../he3 he3_cv_n 0:0.0001:0.05 25.825" using ($1*1000):($3/$1) w l title "p=32.50",\
 "< ../../he3 he3_cv_n 0:0.0001:0.05 26.256" using ($1*1000):($3/$1) w l title "p=29.30",\
 "< ../../he3 he3_cv_n 0:0.0001:0.05 27.323" using ($1*1000):($3/$1) w l title "p=22.22",\
 "< ../../he3 he3_cv_n 0:0.0001:0.05 28.405" using ($1*1000):($3/$1) w l title "p=17.01",\
 "< ../../he3 he3_cv_n 0:0.0001:0.05 30.016" using ($1*1000):($3/$1) w l title "p=11.00",\
 "< ../../he3 he3_cv_n 0:0.0001:0.05 32.568" using ($1*1000):($3/$1) w l title "p=5.040",\
 "< ../../he3 he3_cv_n 0:0.0001:0.05 36.767" using ($1*1000):($3/$1) w l title "p=0.060",\

pause -1