#!/usr/bin/gnuplot

# when writng points I assumed x axis to be 0..1,
# but it is 0.2..1,  x=0.2+x*0.8

set key bottom

plot\
  "greywall86_f18_7.txt" using (0.2+$1*0.8):2 with points pt 6 title "0",\
  "greywall86_f18_8.txt" using (0.2+$1*0.8):2 with points pt 6 title "2.18",\
  "greywall86_f18_9.txt" using (0.2+$1*0.8):2 with points pt 6 title "5.21",\
  "greywall86_f18_4.txt" using (0.2+$1*0.8):2 with points pt 6 title "10.25",\
  "greywall86_f18_5.txt" using (0.2+$1*0.8):2 with points pt 6 title "14.95",\
  "greywall86_f18_6.txt" using (0.2+$1*0.8):2 with points pt 6 title "20.30",\
  "greywall86_f18_1.txt" using (0.2+$1*0.8):2 with points pt 6 title "25.31",\
  "greywall86_f18_2.txt" using (0.2+$1*0.8):2 with points pt 6 title "29.08",\
  "greywall86_f18_3.txt" using (0.2+$1*0.8):2 with points pt 6 title "33.95",\

pause -1